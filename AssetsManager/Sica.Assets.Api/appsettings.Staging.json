{
  "CorsOrigins": [
    "http://localhost:3000"
  ],
  "Authentication": {
    "Authority": "http://localhost:9001",
    "Audience": "sica-assets-api"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Verbose",
    "Properties": {
      "Application": "Assets"
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}